{
  "name": "@company-scope/my-fancy-ui-components",
  "description": "Re-Usable UI components that can be shared across projects",
  "scripts": {
    "test": "yarn run lint && yarn run flow && yarn run jest:coverage",
    "lint": "eslint src",
    "jest": "jest",
    "jest:watch": "yarn run jest --watch",
    "jest:coverage": "yarn run jest --coverage",
    "flow": "flow",
    "flow-typed": "flow-typed",
    "flow-typed-install":
      "flow-typed install --ignoreDeps=bundled peer --overwrite"
  },
  "engines": {
    "node": ">=8.9.2",
    "yarn": ">=1.0.2"
  },
  "keywords": [],
  "dependencies": {
    "@immowelt/babel-preset-immowelt-node": "1.1.0",
    "@immowelt/babel-preset-immowelt-react": "1.2.2"
  },
  "devDependencies": {
    "@immowelt/eslint-config-immowelt-react": "2.0.4",
    "@immowelt/jest-preset-react": "1.1.1",
    "classcat": "3.1.2",
    "empty": "0.10.1",
    "enzyme": "3.4.4",
    "enzyme-adapter-react-16": "1.2.0",
    "enzyme-to-json": "3.3.4",
    "eslint": "5.4.0",
    "flow-bin": "0.69.0",
    "flow-typed": "2.4.0",
    "jest": "23.4.2",
    "prop-types": "15.6.2",
    "react": "16.4.2",
    "react-dom": "16.4.2",
    "react-test-renderer": "16.4.2"
  },
  "peerDependencies": {
    "classcat": "^3.0.0",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0"
  },
  "jest": {
    "preset": "@immowelt/jest-preset-react",
    "transformIgnorePatterns": ["node_modules/(?!@immowelt|@company-scope)"]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/my-user/my-repo.git"
  },
  "version": "0.0.0-development",
  "private": true,
  "license": "my-chosen-spdx-license"
}
