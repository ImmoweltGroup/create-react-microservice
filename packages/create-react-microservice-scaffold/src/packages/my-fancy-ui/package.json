{
  "name": "@company-scope/my-fancy-ui",
  "description": "The universal UI application for the user",
  "scripts": {
    "test": "yarn run lint && yarn run flow && yarn run jest:coverage",
    "lint": "eslint src",
    "jest": "jest",
    "jest:watch": "yarn run jest --watch",
    "jest:coverage": "yarn run jest --coverage",
    "build": "yarn run webpack",
    "dev": "yarn run webpack --watch",
    "webpack":
      "webpack --config ./../my-fancy-ui-config/dist/webpack.config.js",
    "flow": "flow",
    "flow-typed": "flow-typed",
    "flow-typed-install":
      "flow-typed install --ignoreDeps=bundled peer --overwrite"
  },
  "engines": {
    "node": ">=8.9.2",
    "yarn": ">=1.0.2"
  },
  "dependencies": {
    "@immowelt/babel-preset-immowelt-node": "1.1.1",
    "@immowelt/babel-preset-immowelt-react": "1.2.4",
    "@company-scope/my-fancy-ui-components": "0.0.0-development",
    "@company-scope/my-fancy-ui-i18n": "0.0.0-development",
    "@company-scope/my-fancy-ui-logger": "0.0.0-development",
    "classcat": "3.2.3",
    "empty": "0.10.1",
    "hypernova-react": "2.1.0",
    "isomorphic-fetch": "2.2.1",
    "lodash": "4.17.11",
    "node-polyglot": "2.3.0",
    "plow-js": "2.1.0",
    "prop-types": "15.6.2",
    "react": "16.6.3",
    "react-dev-utils": "5.0.3",
    "react-dom": "16.6.3",
    "react-redux": "5.0.7",
    "recompose": "0.30.0",
    "redux": "4.0.1",
    "redux-actions": "2.6.3",
    "redux-lumbergh": "1.0.7",
    "redux-saga": "0.16.2",
    "reselect": "3.0.1",
    "serialize-error": "2.1.0",
    "webpack": "3.12.0",
    "whatwg-fetch": "2.0.4"
  },
  "devDependencies": {
    "@immowelt/eslint-config-immowelt-react": "2.1.1",
    "@immowelt/jest-preset-react": "1.2.1",
    "enzyme": "3.7.0",
    "enzyme-adapter-react-16": "1.7.0",
    "enzyme-to-json": "3.3.5",
    "eslint": "5.9.0",
    "flow-bin": "0.69.0",
    "flow-typed": "2.4.0",
    "jest": "23.6.0",
    "jest-react-redux": "1.0.4",
    "react-test-renderer": "16.6.3",
    "redux-saga-test-plan": "3.7.0"
  },
  "jest": {
    "preset": "@immowelt/jest-preset-react",
    "transformIgnorePatterns": ["node_modules/(?!@immowelt|@company-scope)"]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/my-user/my-repo.git"
  },
  "version": "0.0.0-development",
  "private": true,
  "license": "my-chosen-spdx-license"
}
